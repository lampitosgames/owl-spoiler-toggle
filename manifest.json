{
    "manifest_version": 2,

    "name": "Overwatch League Spoiler Toggle",
    "description": "This extension allows visiters to overwatchleague.com to show/hide match spoilers.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://overwatchleague.com/*",
                "http://overwatchleague.com/*"
            ],
            "js": ["jquery-3.2.1.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Click Here!"
    }
}
